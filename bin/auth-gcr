#!/usr/bin/env bash

echo $CLIENT_SECRET | base64 --decode > ${HOME}/client-secret.json
gcloud auth activate-service-account --key-file ${HOME}/client-secret.json circleci@api-dannydavidson-com.iam.gserviceaccount.com
gcloud config set container/cluster api-dannydavidson-com
gcloud container clusters get-credentials api-dannydavidson-com
gcloud docker -a
